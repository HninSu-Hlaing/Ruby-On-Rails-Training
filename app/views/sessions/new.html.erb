<h1>Sign In</h1>
<%= form_with url:sign_in_path do |f| %>
  <%= f.label :email %>:
  <%= f.text_field :email, class: "form-control" %>
  <%= f.label :password %>:
  <%= f.password_field :password, class: "form-control" %>
  <%= link_to "Forgot your password?" , reset_path %>
  <%= f.submit "Sign in" %>
<% end %>